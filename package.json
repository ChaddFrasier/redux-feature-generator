{
  "name": "redux-feature-generator",
  "version": "0.0.2",
  "description": "Generate redux features including their folders and files that follow the template set by the `react-app --template redux-typescript` API",
  "main": "./dist/cli.js",
  "scripts": {
    "test": "yarn test",
    "build": "tsc",
    "test:gen": "node redux-feature-generator componentName --template redux-typescript",
    "hello": "node . componentName -t redux-typescript"
  },
  "bin": {
    "redux-feature-generator": "./dist/cli.js"
  },
  "files": [
    "/dist/*.js",
    "/dist/types"
  ],
  "keywords": [],
  "author": "chaddfrasier",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^17.0.14",
    "@types/yargs": "^17.0.8",
    "typescript": "^4.5.5",
    "yargs": "^17.3.1"
  },
  "repository": {
    "url": "https://github.com/ChaddFrasier/redux-feature-generator",
    "type": "git"
  }
}
