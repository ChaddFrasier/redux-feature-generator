{
  "name": "redux-feature-generator",
  "version": "1.0.1",
  "description": "Generate Redux feature code from the command line",
  "main": "build/cli.js",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "test": "node build/cli.js customName --template redux-typescript",
    "test:template:fail": "node build/cli.js customName --template x-typescript",
    "test:name:fail": "node build/cli.js customName012",
    "build": "gulp",
    "build:test": "npm run build && npm run test",
    "start": "node build/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChaddFrasier/redux-feature-generator.git"
  },
  "author": "chaddfrasier",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ChaddFrasier/redux-feature-generator/issues"
  },
  "homepage": "https://github.com/ChaddFrasier/redux-feature-generator#readme",
  "bin": {
    "generate-feature": "./build/cli.js"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@types/node": "^17.0.18",
    "del": "^6.0.0",
    "gulp": "^4.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "typescript": "^4.5.5"
  }
}
