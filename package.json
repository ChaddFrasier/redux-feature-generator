{
  "name": "redux-feature-generator",
  "version": "0.0.4",
  "description": "Generate Redux feature code from the command line",
  "main": "build/src/cli.js",
  "files": [
    "build/*",
    "templates/*"
  ],
  "scripts": {
    "test": "node build/src/cli.js customName --template redux-typescript",
    "test:failure:1": "node build/cli.js customName --template x-typescript",
    "build": "gulp",
    "build:test": "npm run build && node build/cli.js",
    "start": "node build/src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChaddFrasier/redux-feature-generator.git"
  },
  "author": "chaddfrasier",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ChaddFrasier/redux-feature-generator/issues"
  },
  "homepage": "https://github.com/ChaddFrasier/redux-feature-generator#readme",
  "bin": {
    "generate-feature": "build/src/cli.js"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "del": "^6.0.0",
    "gulp": "^4.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "minimist": "^1.2.5"
  }
}
